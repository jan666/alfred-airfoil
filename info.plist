				<key>runningsubtext</key>
				<string>Loading...</string>
				<key>script</key>
				<string>tell application "Airfoil"		set res to "&lt;?xml version='1.0'?&gt;&lt;items&gt;"	set aList to every application source		set listThem to {}		repeat with src in aList		set listThem to listThem &amp; {{name of src, id of src}}	end repeat		repeat with src in aList		set srcName to name of src		set srcId to id of src		set itemText to "&lt;item uid=\"" &amp; srcId &amp; "\" arg=\"" &amp; srcId &amp; "\"&gt;&lt;title&gt;" &amp; srcName &amp; "&lt;/title&gt;&lt;subtitle&gt;" &amp; "select " &amp; srcName &amp; "&lt;/subtitle&gt;"		set itemText to itemText &amp; "&lt;icon&gt;airfoil.png&lt;/icon&gt;&lt;/item&gt;"		set res to res &amp; itemText	end repeat		set res to res &amp; "&lt;/items&gt;"	end tell</string>
